{
    "type": "origins:action_over_time",
    "interval": 1,
    "rising_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "function rx.playerdb:api/get_self"
            },
            {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:command",
                    "command": "data modify storage rx:io playerdb.player.data.origins-kombo.check set value [\"P8_1.pling.F#.1\", \"P8_1.pling.F#.1\", \"P8_1.pling.C#\", \"P8_1.pling.G#\", \"P8_1.pling.G#\", \"P8_1.pling.G#\", \"P8_1.pling.G#\", \"P8_1.pling.F#.1\", \"P8_1.pling.G#\", \"P8_1.pling.A#\"]",
                    "comparison": "==",
                    "compare_to": 0
                },
                "if_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:execute_command",
                            "command": "function origins-kombo:internal/cast_success"
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "tellraw @a {\"text\": \"You're gonna have a bad time...\"}"
                        },
                        {
                            "type": "origins:delay",
                            "action": {
                                "type": "origins:execute_command",
                                "command": "kill @s"
                            },
                            "ticks": 40
                        }
                    ]
                },
                "else_action": {
                    "type": "origins:execute_command",
                    "command": "function origins-kombo:internal/cast_fail"
                }
            }
        ]
    },
    "condition": {
        "type": "origins:command",
        "command": "scoreboard players get @s o-k.max_combo.r",
        "comparison": "==",
        "compare_to": 1
    }
}