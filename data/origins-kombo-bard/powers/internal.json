                },
                {
                    "type": "origins:execute_command",
                    "command": "function origins-kombo:internal/reset"
                }
            ]
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_casted",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_cooldown",
                    "comparison": "==",
                    "compare_to": 1
                }
            ]
        }
    },
