{
    "type": "origins:action_over_time",
    "interval": 1,
    "rising_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "function rx.playerdb:api/get_self"
            },
            {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:command",
                    "command": "data modify storage rx:io playerdb.player.data.origins-kombo.check set value [\"P8_1.bass.A\", \"P8_1.bass.D\", \"P8_1.bass.D\", \"P8_1.bass.B\"]",
                    "comparison": "==",
                    "compare_to": 0
                },
                "if_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:execute_command",
                            "command": "function origins-kombo:internal/cast_success"
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "tellraw @a {\"translate\": \"%s casted \\\"Complex Hymn #2\\\"!\", \"color\": \"yellow\", \"with\": [{\"selector\": \"@s\"}]}"
                        },
                        {
                            "type": "origins:trigger_cooldown",
                            "power": "*:kombos/complex_hymn_2/duration"
                        }
                    ]
                },
                "else_action": {
                    "type": "origins:execute_command",
                    "command": "function origins-kombo:internal/cast_fail"
                }
            }
        ]
    },
    "condition": {
        "type": "origins:command",
        "command": "scoreboard players get @s o-k.max_combo.r",
        "comparison": "==",
        "compare_to": 1
    },
    "name": "Complex Hymn #2",
    "description": "A Hymn that would display the 'angry villager' particle around the player. \n\nNote pattern: \nA, D, D, B \n\nKey Signature and Scale: \n A Major \n\nInstrument: \nBass \n\nOctave: 1st"
}